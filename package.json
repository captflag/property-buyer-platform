{
  "name": "property-buyer-platform",
  "version": "1.0.0",
  "description": "A comprehensive customer-facing platform designed to provide property buyers with transparent, real-time insights into their construction projects.",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "server": "node backend/server.js",
    "dev:server": "nodemon backend/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "prepare": "husky install"
  },
  "keywords": ["property", "construction", "real-time", "ai", "platform"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/node": "^18.14.2",
    "typescript": "^4.9.4",
    "vite": "^4.5.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.35.0",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "prettier": "^2.8.4",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.1",
    "nodemon": "^2.0.20",
    "vitest": "^0.28.5"
  }
}